	<queryString>
		<![CDATA[SELECT *
FROM movimientos
	INNER JOIN cuentas ON 
	 movimientos.id_cuenta = cuentas.id_cuenta 
	INNER JOIN tipo_cuenta ON 
	 cuentas.id_tipo_cuenta = tipo_cuenta.id_tipo_cuenta 
WHERE 
	 cuentas.id_tipo_cuenta = 5 OR cuentas.id_tipo_cuenta=4
	 AND movimientos.fecha LIKE '2017%' 
	 ORDER BY tipo_cuenta.id_tipo_cuenta DESC]]>
	</queryString>
